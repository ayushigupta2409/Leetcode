class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        if digits[-1] != 9:
            digits[-1]  = digits[-1] + 1
            return digits
        else:
            for i in range(len(digits)):
                digits[i] = str(digits[i])
            digits = ''.join(digits)
            digits = int(digits) + 1
            return list(str(digits))
